{
    "freeze_paths" : {
        "node_modules/**": ":base64:",
        "node_modules/**/*.svg": ":encodeURIComponent:",
        "*.blocks/**": ":base64:",
        "*.blocks/**/*.svg": ":encodeURIComponent:",
        "*.blocks/**/fonts/**": "static/fonts",
        "*.blocks/**/images/**": "static/images"
    },
    "freeze_nesting_level": 1
}
